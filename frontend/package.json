{
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "nuxt build --dotenv .config/prod.env",
    "buildStaging": "nuxt build --dotenv .config/staging.env",
    "buildLocal": "nuxt build --dotenv .config/local.env",
    "dev": "nuxt dev --port 3333 --dotenv .config/local.env",
    "devStaging": "nuxt dev --port 3333 --dotenv .config/localStaging.env",
    "generate": "nuxt generate",
    "preview": "bun --import=./.output/server/sentry.server.config.mjs --env-file=./.config/local.env run ./.output/server/index.mjs",
    "previewBuild": "nuxt build && nuxt preview",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier --list-different -w .",
    "lint:oxlint": "oxlint -c .oxlint.json --import-plugin --fix",
    "lint:typecheck": "nuxt typecheck",
    "prepare": "nuxt prepare && cd .. && husky install frontend/.husky"
  },
  "lint-staged": {
    "*.{ts,js,vue,html}": [
      "oxlint --fix",
      "prettier -c",
      "eslint"
    ]
  },
