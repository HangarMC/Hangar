    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "previewBuild": "nuxt build && nuxt preview",
    "lint:eslint": "eslint --ext \".js,.vue,.ts,.html\" --ignore-path .gitignore --fix .",
